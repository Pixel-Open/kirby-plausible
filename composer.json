{
    "name": "pixelopen/kirby-plausible",
    "type": "kirby-plugin",
    "description": "A plugin for Kirby CMS to add statistic from plausible.io",
    "homepage": "https://github.com/Pixel-Open/kirby-plausible",
    "license": "MIT",
    "authors": [
        {
            "name": "Pixel Open",
            "homepage": "https://pixel-open.org/"
        }
    ],
    "keywords": [
        "kirby",
        "kirby-cms",
        "kirby-plugin"
    ],
    "require": {
        "getkirby/composer-installer": "^1.1",
        "getkirby/cms": "^4.0.0||^5.0.0"
    },
    "extra": {
        "installer-name": "kirby-plausible"
    },
    "require-dev": {
        "symplify/easy-coding-standard": "^12.0",
        "phpstan/phpstan": "^1.10"
    },
    "config": {
        "allow-plugins": {
            "getkirby/composer-installer": true
        }
    }
}
